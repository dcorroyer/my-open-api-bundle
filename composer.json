{
    "name": "dcorroyer/my-rest-bundle",
    "description": "A REST API helper bundle for my symfony apps",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "My\\RestBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "My\\RestBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "nelmio/api-doc-bundle": "^4.12",
        "symfony/http-foundation": "^6.3",
        "symfony/serializer": "6.3.*",
        "knplabs/knp-paginator-bundle": "^6.2",
        "symfony/validator": "^6.3",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.16"
    },
    "require-dev": {
        "zenstruck/foundry": "^1.36"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "sort-packages": true
    }
}
