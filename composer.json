{
    "name": "dcorroyer/my-rest-bundle",
    "description": "A rest api helper bundle for my symfony apps",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "My\\RestBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "My\\RestBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "nelmio/api-doc-bundle": "^4.12",
        "symfony/http-foundation": "^6.3",
        "symfony/serializer": "6.3.*",
        "symfony/framework-bundle": "6.3.x-dev",
        "knplabs/knp-paginator-bundle": "^6.2",
        "symfony/validator": "6.4.x-dev",
        "symfony/orm-pack": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
